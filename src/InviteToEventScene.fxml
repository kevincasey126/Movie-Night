<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="150.0" maxWidth="450.0" minHeight="150.0" minWidth="450.0" prefHeight="150.0" prefWidth="450.0" style="-fx-background-color: #cde3f1;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <VBox prefHeight="150.0" prefWidth="105.0">
      <children>
        <Button fx:id="back_button" mnemonicParsing="false" onAction="#pressedBackButton" prefHeight="38.0" prefWidth="41.0" style="-fx-background-color: #3892C7;" text="X" textFill="#000088">
          <font>
            <Font name="Berlin Sans FB Demi Bold" size="20.0" />
          </font>
          <VBox.margin>
            <Insets />
          </VBox.margin>
        </Button>
      </children>
    </VBox>
    <VBox layoutX="112.0" prefHeight="111.0" prefWidth="334.0">
      <children>
        <Label prefHeight="54.0" prefWidth="192.0" text="Add Attendee" textFill="#000088" underline="true">
          <font>
            <Font name="Berlin Sans FB Demi Bold" size="24.0" />
          </font></Label>
        <TextField fx:id="username_textfield" onAction="#sendEventRequest" prefHeight="25.0" prefWidth="355.0" promptText="Enter user name here..." />
        <Label fx:id="response_label" prefHeight="33.0" prefWidth="487.0" />
      </children>
      <padding>
        <Insets right="5.0" />
      </padding>
    </VBox>
  </children>
</AnchorPane>
